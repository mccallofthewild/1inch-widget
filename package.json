{
	"name": "my-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"typegen:graphql": "graphql-codegen --config codegen.yml",
		"typegen:swagger": "npx swagger-typescript-api -p ./swagger.json -o ./generated -n OneInchSwagger.ts --no-client --route-types && node -e \"fs.writeFileSync('./generated/OneInchSwagger.ts', 'export namespace OneInchSwagger { ' + fs.readFileSync('./generated/OneInchSwagger.ts') + '}')\"",
		"typegen:swagger:2": "npx @openapitools/openapi-generator-cli generate -i ./swagger.json -g typescript-fetch -o ./generated/openapi",
		"typegen:contracts": "yarn run typechain --target ethers-v5 --outDir ./generated/contracts ./abis/**/*.json"
	},
	"dependencies": {
		"@web3-react/core": "^6.1.1",
		"@web3-react/injected-connector": "^6.0.7",
		"date-fns": "^2.11.1",
		"ethers": "^5.0.24",
		"gray-matter": "^4.0.2",
		"next": "^10.0.0",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"remark": "^12.0.0",
		"remark-html": "^12.0.0",
		"graphql": "^15.4.0"
	},
	"devDependencies": {
		"@types/node": "^14.14.14",
		"@types/react": "^17.0.0",
		"typescript": "^4.1.3",
		"@graphql-codegen/add": "^2.0.2",
		"@graphql-codegen/cli": "1.19.4",
		"@graphql-codegen/introspection": "1.18.1",
		"@graphql-codegen/typescript": "1.19.0",
		"@graphql-codegen/typescript-resolvers": "^1.18.0",
		"typechain": "^4.0.1"
	}
}
